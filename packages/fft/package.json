{
  "name": "@applemusic-like-lyrics/fft",
  "collaborators": [
    "SteveXMH <39523898+Steve-xmh@users.noreply.github.com>"
  ],
  "description": "一个音频可视化模块，用于将传入的音频原始波形数据转换成便于观赏显示的频谱",
  "version": "0.1.1",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Steve-xmh/applemusic-like-lyrics"
  },
  "scripts": {
    "build": "wasm-pack build --release",
    "build:dev": "wasm-pack build --dev",
    "fmt": "cargo fmt",
    "preinstall": "npx only-allow pnpm"
  },
  "files": [
    "pkg/*.wasm",
    "pkg/*.js",
    "pkg/*.d.ts"
  ],
  "module": "pkg/fft.js",
  "types": "pkg/fft.d.ts",
  "sideEffects": [
    "./pkg/fft.js",
    "./pkg/snippets/*"
  ]
}
